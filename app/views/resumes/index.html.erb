<h1>Listing resumes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Dob</th>
    <th>Qulifaction</th>
    <th>Experince</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @resumes.each do |resume| %>
  <tr>
    <td><%= resume.name %></td>
    <td><%= resume.DOB %></td>
    <td><%= resume.qulifaction %></td>
    <td><%= resume.experince %></td>
    <td><%= resume.description %></td>
    <td><%= link_to 'Show', resume %></td>
    <td><%= link_to 'Edit', edit_resume_path(resume) %></td>
    <td><%= link_to 'Destroy', resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

